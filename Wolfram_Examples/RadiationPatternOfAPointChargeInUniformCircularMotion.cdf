Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    StyleBox["E",
     FontWeight->"Bold",
     FontSlant->"Plain"], "(", 
    RowBox[{
     StyleBox["r",
      FontWeight->"Bold",
      FontSlant->"Plain"], ",", "t"}], ")"}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["Q", 
      RowBox[{"4", "\[Pi]", " ", 
       SubscriptBox["\[CurlyEpsilon]", "0"]}]], 
     RowBox[{"{", 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           StyleBox["R",
            FontWeight->"Bold",
            FontSlant->"Plain"], "-", 
           StyleBox[
            RowBox[{
             StyleBox["R",
              FontSlant->"Plain"], "\[CenterDot]", "\[Beta]"}],
            FontWeight->"Bold"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["\[Beta]", "2"]}], ")"}]}], 
        SuperscriptBox[
         RowBox[{
          StyleBox["(",
           FontWeight->"Bold",
           FontSlant->"Plain"], 
          RowBox[{
           StyleBox["R",
            FontWeight->"Bold",
            FontSlant->"Plain"], "-", 
           RowBox[{
            StyleBox["R",
             FontWeight->"Bold",
             FontSlant->"Plain"], "\[CenterDot]", 
            StyleBox["\[Beta]",
             FontWeight->"Bold"]}]}], ")"}], "3"]], "+", 
       FractionBox[
        RowBox[{
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "\[Cross]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             StyleBox["R",
              FontWeight->"Bold",
              FontSlant->"Plain"], "-", 
             StyleBox[
              RowBox[{
               StyleBox["R",
                FontSlant->"Plain"], "\[CenterDot]", "\[Beta]"}],
              FontWeight->"Bold"]}], ")"}], "\[Cross]", 
           StyleBox["\[Alpha]",
            FontWeight->"Bold"]}], ")"}]}], 
        RowBox[{"c", " ", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            StyleBox["R",
             FontWeight->"Bold",
             FontSlant->"Plain"], "-", 
            RowBox[{
             StyleBox["R",
              FontWeight->"Bold",
              FontSlant->"Plain"], "\[CenterDot]", 
             StyleBox["\[Beta]",
              FontWeight->"Bold"]}]}], ")"}], "3"]}]]}], "}"}]}], "=", 
    RowBox[{
     RowBox[{
      StyleBox[
       SubscriptBox[
        StyleBox["E",
         FontWeight->"Bold",
         FontSlant->"Plain"], 
        StyleBox["v",
         FontSlant->"Plain"]],
       FontWeight->"Bold"], "(", 
